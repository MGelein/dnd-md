{
    "manifest_version": 2,
   
    "name": "DND MD",
    "version": "0.1.0",
    "description": "Visualizing Markdown of DND",
   
    "content_scripts": [{
      "css": [ "css/bootstrap.min.css", "css/style.css"],
      "js": ["js/hook.js"],
      "matches": ["file://*.md"]
    }],
    "web_accessible_resources": [
        "js/marked.js",
        "js/changer.js",
        "js/jq.js"
      ]
  }